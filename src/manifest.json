{
	"name": "Browse By command",
	"version": "1.0",
	"description": "Browse the internet with the command line like a boss",
	"manifest_version": 2,
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"icons": {
		"128": "icon-128.png"
	},
	"background": {
		"scripts": ["events.bundle.js"],
		"persistent": true
	},
	"permissions": [
	  "http://*/",
	  "https://*/"
  ],
	"commands": {
		"browseByCommand": {
			"suggested_key": {
				"default": "Ctrl+Shift+K",
				"mac": "Command+Shift+K"
			},
			"description": "Toggle feature foo"
		},
		"focus": {
			"suggested_key": {
				"default": "Ctrl+Shift+L",
				"mac": "Command+Shift+L"
			},
			"description": "Toggle feature foo"
		}
	}
}
